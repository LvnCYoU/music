scribed ? '已收藏' : '收藏'\r\n    }\r\n  },\r\n  watch: {},\r\n  methods: {\r\n    // 播放歌曲\r\n    playSong(item, index) {\r\n      this.selectPlay({\r\n        list: this.songs,\r\n        index\r\n      })\r\n    },\r\n    // 停止播放歌曲\r\n    pauseSong() {\r\n      this.pausePlay()\r\n    },\r\n    // 播放全部\r\n    playAllSong() {\r\n      this.playAll({\r\n        list: this.songs\r\n      })\r\n    },\r\n    // 收藏歌单\r\n    async collect() {\r\n      this.$emit('collectArtist')\r\n    },\r\n    ...mapActions([\r\n      // 点击选择播放\r\n      'selectPlay',\r\n      // 点击播放全部\r\n      'playAll',\r\n      'pausePlay'\r\n    ])\r\n  },\r\n  created() {},\r\n  mounted() {}\r\n}\r\n</script>\r\n<style lang=\"stylus\" scoped>\r\n.artist-box >>> tbody img {\r\n  border-radius: 5px;\r\n}\r\n.artist-box {\r\n  width: 100%;\r\n  .tool-head {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: flex-end;\r\n    margin: 15px 0;\r\n    .item {\r\n      background: #f2f2f2;\r\n      color: #333333;\r\n      padding: 7px 15px;\r\n      border-radius: 50px;\r\n      cursor: pointer;\r\n      margin-left: 15px;\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      transition: all 0.4s;\r\n      i {\r\n        margin-right: 5px;\r\n      }\r\n      .nicefenxiang1 {\r\n        font-size: 18px;\r\n      }\r\n      &.active {\r\n        background: $color-theme;\r\n        color: #fff;\r\n      }\r\n    }\r\n    .play-item {\r\n      background: $color-theme;\r\n      color: #fff;\r\n    }\r\n  }\r\n  .artist-table {\r\n    width: 100%;\r\n    thead {\r\n      width: 100%;\r\n      height: 50px;\r\n      line-height: 50px;\r\n      background: #fafafa;\r\n      color: #999;\r\n      th {\r\n        padding: 0 9px;\r\n        text-align: left;\r\n        font-weight: 300;\r\n        white-space: nowrap;\r\n        overflow: hidden;\r\n        -o-text-overflow: ellipsis;\r\n        text-overflow: ellipsis;\r\n        &.th-index {\r\n    