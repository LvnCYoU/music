AA,IAAA,CAAA,IAAA,CAAA,KAAA,GAAA,EAAA;;AACA,UAAA,UAAA,CAAA,KAAA,KAAA,CAAA,KAAA,MAAA,EAAA,IAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,aAAA,QAAA,GADA,CACA;;AACA,aAAA,QAAA,GAAA,KAAA,MAAA;AACA,aAAA,MAAA;AACA;AACA;AAhBA,GAvBA;AAyCA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA;AADA,GAzCA;AA4CA,EAAA,OA5CA,qBA4CA;AACA,SAAA,IAAA;AACA,GA9CA;AA+CA,EAAA,OAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,QAAA;AACA,KAHA;AAKA,IAAA,QALA,sBAKA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,IAAA,CAAA,KAAA,CAAA,MAAA,CAAA,MAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,KAAA,GAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,KAAA,CAAA;;AACA,sBAAA,CAAA,MAAA,CAAA,KAAA,CAAA,MAAA,EAAA;AACA,wBAAA,GAAA,GAAA,EAAA;AACA,oBAAA,GAAA,CAAA,KAAA,GAAA,WAAA;;AACA,oBAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,iBARA,CADA;;AAAA;AAUA,gBAAA,MAAA,CAAA,GAAA,GAAA,MAAA,CAAA,KAAA,CAAA,MAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,KAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAjBA;AAmBA;AACA,IAAA,QApBA,sBAoBA;AACA,UAAA,KAAA,KAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,UAAA,EAAA,GAAA,KAAA,KAAA,CAAA,SAAA,CAFA,CAGA;;AACA,UAAA,KAAA,MAAA,GAAA,KAAA,KAAA,EAAA;AACA,QAAA,EAAA,CAAA,KAAA,CAAA,SAAA,GAAA,gBAAA,CAAA,KAAA,MAAA,GAAA,KAAA,KAAA,IAAA,KAAA,MAAA,GAAA,KAAA,CADA,CACA;AACA,OAFA,MAGA,IAAA,KAAA,MAAA,GAAA,KAAA,KAAA,EAAA;AACA,QAAA,EAAA,CAAA,KAAA,CAAA,SAAA,GAAA,eAAA,CADA,CACA;AACA;AACA,KA9BA;AAgCA;AACA,IAAA,cAjCA,4BAiCA;AACA,WAAA,KAAA,CAAA,gBAAA,CAAA,YAAA,EAAA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAFA;AAGA;AArCA;AA/CA,CAAA","sourcesContent":["<template>\r\n  <transition name=\"fade\" mode=\"out-in\">\r\n    <div \r\n      class=\"lyric-box shadow\" \r\n      @wheel.stop.prevent\r\n    >\r\n      <scroll \r\n        class=\"lyric\" \r\n        :data=\"lyric\"\r\n        ref=\"lyricList\"\r\n      >\r\n        <div class=\"wrapper\" ref=\"lyricWrap\" style=\"transition-duration='1000ms'\">\r\n          <p \r\n            v-for=\"(item,index) in lyric\" \r\n            :key=\"index\" ref=\"line\"\r\n            :class=\"lineNo-1 == index ? 'is-linehigh' : ''\"\r\n          >\r\n            {{ item.lyric }}\r\n          </p>\r\n        </div>\r\n      </scroll>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nimport {mapState} from 'vuex'\r\nlet Scroll = () => import('../../../api/scroll');\r\nexport default {\r\n  props: {\r\n    show: {\r\n      type: Boolean,\r\n    },\r\n    time: {\r\n      type: Number,\r\n      required: true,\r\n    }\r\n  },\r\n  data(){\r\n    return{\r\n      lyric: [],\r\n      lineNo: 0, // 当前行\r\n      propLine: null, // 上一行\r\n      C_pos: 6, // C位\r\n      offset: -34, // 滚动距离\r\n      len: 0,\r\n    }\r\n  },\r\n  computed:{\r\n    ...mapState(['songId','audio']),\r\n  },\r\n  watch: {\r\n    show(val){\r\n      if(val){\r\n        this.$nextTick( () =>{\r\n          this.$refs.lyricList.refresh();\r\n        })\r\n      }\r\n    },\r\n    time(){\r\n      if( this.lineNo == this.len) return;\r\n      if(this.len == 1 || !this.len) return;\r\n      if(parseFloat(this.lyric[this.lineNo].time) <= this.time){\r\n        this.lineHigh(); //高亮当前行\r\n        this.propLine = this.lineNo;\r\n        this.lineNo++;\r\n      }\r\n    }\r\n  },\r\n  components: {\r\n    Scroll,\r\n  },\r\n  mounted(){\r\n    this.init();\r\n  },\r\n  methods: {\r\n    init(){\r\n      this.getLyric();\r\n    },\r\n\r\n    async getLyric(){\r\n      await this.$api.Lyric(this.songId)\r\n        .then(res => {\r\n          this.lyric = this.utils.Lyric(res.data.lrc.lyric)\r\n          if(!this.lyric.length){\r\n            let arr = [];\r\n            arr.lyric = '暂无歌词，敬请见谅';\r\n            this.lyric.push(arr);\r\n          }\r\n        })\r\n        this.len = this.lyric.length;\r\n        console.log(this.lyric)\r\n    },\r\n\r\n    // 高亮当前歌词\r\n    lineHigh() {\r\n      if(this.lyric.length == 1) return ;\r\n      let ul = this.$refs.lyricWrap;\r\n      //文字滚动\r\n      if(this.lineNo > this.C_pos){\r\n        ul.style.transform = \"translateY(\" + (this.lineNo - this.C_pos) * this.offset + \"px)\"; //整体向上滚动一行高度\r\n      }\r\n      else if(this.lineNo < this.C_pos){\r\n        ul.style.transform = \"translateY(0)\"; //回到开头\r\n      }\r\n    },\r\n\r\n    // 获取当前播放时长\r\n    getCurrentTime(){\r\n      this.audio.addEventListener('timeupdate', () => {\r\n        console.log(1)\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n  .lyric-box{\r\n    position: absolute;\r\n    right: 0;\r\n    bottom: 80px;\r\n    padding: 44px 30px;\r\n    width: 360px;\r\n    height: 500px;\r\n    border-radius: 3px;\r\n    background: #fff;\r\n    box-sizing: border-box; \r\n    user-select: none;\r\n    z-index: 999;\r\n    .lyric{\r\n      height: 100%;\r\n      overflow: hidden;\r\n      transition-duration: 1s;\r\n      .wrapper{\r\n        font-size: 12px;\r\n        transition-duration: 600ms;\r\n        p{\r\n          padding: 5px 0;\r\n          line-height: 24px;\r\n          font-size: 14px;\r\n          font-weight: 300;\r\n          text-align: center;\r\n          &.is-linehigh{\r\n            color: $mainColor;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n</style>"],"sourceRoot":"src/components/player/player-lyric"}]}