AVA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA,KAtDA;AAwDA,IAAA,OAxDA,mBAwDA,IAxDA,EAwDA;AAAA;;AACA,QAAA,KAAA,GAAA;;AACA,UAAA,KAAA,GAAA,GAAA,EAAA,EAAA;AACA,aAAA,SAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAFA;AAGA;;AACA,MAAA,IAAA,CAAA,IAAA,GAAA,KAAA;AACA,KAhEA;AAkEA,IAAA,UAlEA,sBAkEA,GAlEA,EAkEA,GAlEA,EAkEA;AAAA;;AACA,UAAA,OAAA,GAAA;AACA,gBAAA;AAAA,iBAAA,MAAA,CAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AAAA,SADA;AAEA,mBAAA;AAAA,iBAAA,MAAA,CAAA,MAAA,CAAA,OAAA,GAAA,GAAA;AAAA,SAFA;AAGA,gBAAA;AAAA,iBAAA,MAAA,CAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AAAA;AAHA,OAAA;AAKA,MAAA,OAAA,WAAA,GAAA,EAAA,CAAA,IAAA,CAAA,IAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,MAAA,CAAA,MAAA,GAAA,CAAA;AACA,WAAA,UAAA,CAAA,KAAA,MAAA;AACA,WAAA,SAAA,CAAA,KAAA,MAAA;AACA,WAAA,MAAA,CAAA,MAAA,GAAA,CAAA;AACA,KA9EA;AAgFA,IAAA,IAhFA,kBAgFA;AAAA;;AACA,WAAA,UAAA,IAAA,UAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,MAAA,IAAA,MAAA,CAAA,MAAA,CAAA,KAAA;;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,MAAA,CAAA,MAAA;AACA,OAHA,EAGA,IAHA,CAAA;AAIA,WAAA,UAAA,GAAA,IAAA;AACA;AAtFA;AApEA,CAAA","sourcesContent":["<template>\r\n  <div id=\"singer\">\r\n    <div class=\"container\">\r\n      <div class=\"filter\">\r\n        <ul class=\"tag-lang\">\r\n          <li \r\n          :class=\"item.type == params.area ? 'is-active' : ''\"\r\n          v-for=\"item in area\" \r\n          :key=\"item.type\"\r\n          @click=\"chooseType('area',item.type)\"\r\n          ref=\"area\"\r\n          >\r\n            {{ item.name }}\r\n          </li>\r\n        </ul>\r\n        <ul class=\"tag-type\">\r\n          <li v-for=\"item in type\" \r\n          :class=\"item.type == params.type ? 'is-active' : ''\"\r\n          :key=\"item.type + item.name\"\r\n          @click=\"chooseType('type',item.type)\"\r\n          ref=\"sex\"\r\n          >\r\n            {{ item.name }}\r\n          </li>\r\n        </ul>\r\n        <ul class=\"tag-en\">\r\n          <li \r\n          :class=\"params.initial == item.type ? 'is-active' : ''\"\r\n          v-for=\"item in letter\" \r\n          :key=\"item.type+item.name\"\r\n          @click=\"chooseType('initial',item.type)\"\r\n          ref=\"lang\"\r\n          >\r\n            {{ item.name }}\r\n          </li>\r\n        </ul>\r\n      </div>\r\n      <div class=\"load-more\" \r\n        ref=\"loading\"\r\n        v-loading=\"loading\"\r\n        element-loading-text=\"拼命加载中\"\r\n        element-loading-background=\"#fff\"\r\n        :data=\"artist\"\r\n        style=\"height: 100%; width: 100%\"\r\n        >\r\n        <ul class=\"singer-list\">\r\n          <li \r\n          v-for=\"(item,index) in artist\"\r\n          :key=\"item.id + item.name\">\r\n            <router-link \r\n            tag=\"a\" \r\n            :to=\"{name:'singer-detail',query:{id:item.id}}\"\r\n            >\r\n              <div class=\"cover\">\r\n                <div class=\"img\">\r\n                  <el-image \r\n                    :src=\"item.imgUrl\" \r\n                    fit=\"fill\"  \r\n                    @load=\"loadImg(item)\"\r\n                    v-loading=\"item.load\"\r\n                    :data=\"artist[index].imgUrl\"\r\n                    element-loading-background=\"#fff\"\r\n                  ></el-image>\r\n                </div>\r\n              </div>\r\n              <div class=\"info\">\r\n                <p class=\"name\">{{ item.name }}</p>\r\n                <p class=\"count\">单曲数{{ item.music }}</p>\r\n              </div>\r\n            </router-link>\r\n          </li>\r\n        </ul>\r\n        <push-load v-show=\"loadStatus\" @scroll-data='load'/>\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapActions} from 'vuex';\r\nimport pushLoad from '../push-load';\r\nexport default {\r\n  data(){\r\n    return{\r\n      img: 0,\r\n      artist: [],\r\n      loading: true,\r\n      loadStatus: true,\r\n      // 歌手筛选信息值\r\n      area: [\r\n        {\r\n          name: '全部',\r\n          type: -1,\r\n        },\r\n        {\r\n          name: '华语',\r\n          type: 7,\r\n        },\r\n        {\r\n          name: '欧美',\r\n          type: 96\r\n        },\r\n        {\r\n          name: '日本',\r\n          type: 8\r\n        },\r\n        {\r\n          name: '韩国',\r\n          type: 16\r\n        },\r\n        {\r\n          name: '其他',\r\n          type: 0\r\n        }\r\n      ],\r\n      type: [\r\n        {\r\n          name: '全部',\r\n          type: -1\r\n        },\r\n        {\r\n          name: '男歌手',\r\n          type: 1\r\n        },\r\n        {\r\n          name: '女歌手',\r\n          type: 2\r\n        },\r\n        {\r\n          name: '乐队',\r\n          type: 3\r\n        }\r\n      ],\r\n      letter: [],\r\n      params: {\r\n        limit: 40,\r\n        offset: 0,\r\n        initial: -1,\r\n        type: -1,\r\n        area: -1,\r\n      }\r\n    }\r\n  },\r\n  async mounted(){\r\n    await this.init();\r\n  },\r\n  components: {\r\n    pushLoad,\r\n  },\r\n  methods: {\r\n    ...mapActions(['singerInit']),\r\n\r\n    init(){ \r\n      this.$nextTick( () => {\r\n        if(this.$store.state.singer.area){\r\n          this.params = this.$store.state.singer;\r\n          this.params.offset = 0;\r\n        }\r\n        this.getLetter();\r\n        this.getSinger(this.params);\r\n      })\r\n    },\r\n\r\n    // a-z 字母获取\r\n    getLetter(){\r\n      let ens = [];\r\n      ens.push({\r\n        name: '热门',\r\n        type: -1,\r\n      })\r\n      for(let i = 0; i < 26; i++){\r\n        ens.push({\r\n          name: String.fromCharCode(65 + i),\r\n          type: String.fromCharCode(97 + i)\r\n        })\r\n      }\r\n      ens.push({\r\n        name: '其他',\r\n        type: 0\r\n      })\r\n      this.letter = ens;\r\n    },\r\n\r\n    // 获取歌手列表\r\n    async getSinger(params){\r\n      let arrSinger = [];\r\n      this.singerInit(params);\r\n      await this.$api.ArtistList(params)\r\n        .then( res => {\r\n          arrSinger = res.data.artists;\r\n          console.log(res)\r\n        })\r\n      arrSinger.map( list => {\r\n        let obj = {\r\n          id: list.id,\r\n          imgUrl: list.picUrl,\r\n          name: list.name,\r\n          music: list.musicSize,\r\n          load: true,\r\n        };\r\n        this.artist.push(obj);\r\n        // this.loadStatus = false;\r\n      })\r\n    },\r\n\r\n    loadImg(item){\r\n      ++this.img;\r\n      if(this.img > 30){\r\n        this.$nextTick( () => {\r\n          this.loading = false;\r\n        })\r\n      }\r\n      item.load = false\r\n    },\r\n\r\n    chooseType(str,val){\r\n      let actions = {\r\n        'area': () => this.params.area = val,\r\n        'initial': () => this.params.initial = val,\r\n        'type' : () => this.params.type = val,\r\n      }\r\n      actions[`${str}`].call(this)\r\n      this.loading = true;\r\n      this.params.offset = 0;\r\n      this.singerInit(this.params);\r\n      this.getSinger(this.params);\r\n      this.artist.length = 0;\r\n    },\r\n\r\n    load(){\r\n      this.loadStatus && setTimeout( () => {\r\n        this.params.offset += this.params.limit;\r\n        this.getSinger(this.params);\r\n      },1000)\r\n      this.loadStatus = true;\r\n    }\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n  .filter{\r\n    ul{\r\n      margin: 20px 0;\r\n      display: flex;\r\n      \r\n      li{\r\n        margin-right: 6px;\r\n        width: 56px;\r\n        height: 28px;\r\n        line-height: 29px;\r\n        font-size: 13px;\r\n        text-align: center;\r\n        border-radius: 14px;\r\n        cursor: pointer;\r\n\r\n        &.is-active{\r\n          background: #fa2800;\r\n          font-weight: 700;\r\n          color: #fff;\r\n        }\r\n\r\n        \r\n      }\r\n    }\r\n    .tag-en{\r\n      margin: 10px 0 0;\r\n      li{\r\n        width: 28px;\r\n        height: 28px;\r\n        margin-right: 14px;\r\n        text-align: center;\r\n        line-height: 29px;\r\n        border-radius: 50%;\r\n        font-size: 13px;\r\n        color: #333;\r\n        cursor: pointer;\r\n\r\n        &:first-child,&:last-child{\r\n          width: 56px;\r\n          border-radius: 14px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .load-more{\r\n    ul{\r\n      margin: 30px 15px 0;\r\n      display: flex;\r\n      flex-wrap: wrap;\r\n      li{\r\n        padding: 0 15px 30px;\r\n        flex: 0 0 10%;\r\n        max-width: 10%;\r\n\r\n        .cover{\r\n          position: relative;\r\n          z-index: 2;\r\n          padding-top: 100%;\r\n          border-radius: 50%;\r\n          // background-color: #d9d9d9;\r\n          .img{\r\n            position: absolute;\r\n            top: 0;\r\n            left: 0;\r\n            overflow: hidden;\r\n            width: 100%;\r\n            height: 100%;\r\n            border-radius: 50%;\r\n          }\r\n          .el-image{\r\n            height: 100%;\r\n          }\r\n          \r\n        }\r\n        .info{\r\n          margin-top: 15px;\r\n          text-align: center;\r\n          .name{\r\n            font-weight: 700;\r\n            font-size: 14px;\r\n            line-height: 18px;\r\n            word-break: break-word;\r\n          }\r\n          .count{\r\n            font-size: 12px;\r\n            margin-top: 10px;\r\n            color: #fa2800;\r\n          }\r\n        }\r\n\r\n        &:hover{\r\n          .info{\r\n            .name{\r\n              color: #fa2800;\r\n            }\r\n            \r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n  \r\n</style>"],"sourceRoot":"src/components/common/singer"}]}