SAAA,IAAA;AACA,GArBA;AAsBA,EAAA,OAAA,EAAA;AACA,IAAA,IADA,kBACA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,MAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,OAAA;;AACA,QAAA,KAAA,CAAA,cAAA;;AACA,QAAA,KAAA,CAAA,WAAA;;AACA,QAAA,KAAA,CAAA,aAAA;AACA,OALA;AAOA,KAVA;AAYA;AACA,IAAA,OAbA,qBAaA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,IAAA,CAAA,OAAA,CAAA,MAAA,CAAA,EAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA;AACA,sBAAA,MAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA;;AACA,kBAAA,MAAA,CAAA,aAAA,CAAA,MAAA;AACA,iBALA,CADA;;AAAA;AAOA,gBAAA,MAAA,CAAA,IAAA,GAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,CAAA,EAAA,EAAA,CAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KArBA;AAuBA;AACA,IAAA,cAxBA,4BAwBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,EADA;AAEA,kBAAA,KAAA,EAAA;AAFA,iBADA;AAAA;AAAA,uBAKA,MAAA,CAAA,IAAA,CAAA,WAAA,CAAA,MAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,SAAA;AACA,iBAHA,CALA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAjCA;AAmCA;AACA,IAAA,WApCA,yBAoCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,IAAA,CAAA,QAAA,CAAA,MAAA,CAAA,EAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,EAAA,GAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,KAAA,CAAA,CAAA,EAAA,CAAA,CAAA;AACA,iBAHA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KAzCA;AA2CA;AACA,IAAA,aA5CA,2BA4CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,IAAA,CAAA,UAAA,CAAA,MAAA,CAAA,EAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,kBAAA,MAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CAAA,KAAA,CAAA,CAAA,EAAA,CAAA,CAAA;AACA,iBAJA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,KAlDA;AAoDA;AACA,IAAA,aArDA,yBAqDA,MArDA,EAqDA;AACA,WAAA,IAAA,GAAA;AACA,QAAA,IAAA,EAAA,MAAA,CAAA,IADA;AAEA,QAAA,MAAA,EAAA,MAAA,CAAA,MAFA;AAGA,QAAA,IAAA,EAAA,MAAA,CAAA,SAHA;AAIA,QAAA,KAAA,EAAA,MAAA,CAAA,SAJA;AAKA,QAAA,KAAA,EAAA,MAAA,CAAA,SALA;AAMA,QAAA,EAAA,EAAA,MAAA,CAAA;AANA,OAAA;AAQA,KA9DA;AAgEA;AACA,IAAA,IAjEA,gBAiEA,GAjEA,EAiEA;AACA,MAAA,QAAA,CAAA,aAAA,YAAA,GAAA,GAAA,cAAA,CAAA,IAAA;AACA;AAnEA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"singer-detail\">\r\n    <div class=\"container\">\r\n      <div class=\"singer-info\">\r\n        <div class=\"cover\">\r\n          <el-image\r\n            :src=\"info.picUrl\"\r\n            fit=\"cover\"\r\n          >\r\n          </el-image>\r\n        </div>\r\n        <div class=\"info\">\r\n          <div class=\"name\">\r\n            <h1>{{ info.name }}</h1>\r\n          </div>\r\n          <div class=\"des\">\r\n            <span>{{ info.desc }}</span>\r\n          </div>\r\n          <ul class=\"works\">\r\n            <li class=\"item\">\r\n              <span @click=\"jump('song')\">单曲 <strong>{{ info.music }}</strong></span>\r\n            </li>\r\n            <li class=\"item\">\r\n              <span @click=\"jump('album')\">专辑 <strong>{{ info.album }}</strong></span>\r\n            </li>\r\n            <li class=\"item\">\r\n              <span @click=\"jump('mv')\">MV <strong>{{ info.mv }}</strong></span>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n      <div class=\"song\" id=\"song\">\r\n        <header class=\"title\">\r\n          <h2>热门歌曲</h2>\r\n        </header>\r\n        <song-list :songLists=\"songList\" />\r\n      </div>\r\n      <div class=\"album\" id=\"album\">\r\n        <header class=\"title\">\r\n          <h2>专辑</h2>\r\n        </header>\r\n        <album :list=\"album\" />\r\n      </div>\r\n      <div class=\"mv\" id=\"mv\">\r\n        <header class=\"title\">\r\n          <h2>MV</h2>\r\n        </header>\r\n        <mv :list=\"mv\" />\r\n      </div>\r\n      <div class=\"simi\">\r\n        <header class=\"title\">\r\n          <h2>相似歌手</h2>\r\n        </header>\r\n        <singer :artist=\"simi\" />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SongList from '../../song-list'\r\nimport Album from '../../album'\r\nimport Singer from '../../singer/singer'\r\nimport Mv from '../../MV/mv'\r\nexport default {\r\n  data(){\r\n    return{\r\n      id: this.$route.query.id,\r\n      info: {},\r\n      song: [],\r\n      songList: [],\r\n      album: [],\r\n      artist: [],\r\n      mv: [],\r\n      simi: [],\r\n    }\r\n  },\r\n  components:{\r\n    SongList,\r\n    Album,\r\n    Singer,\r\n    Mv,\r\n  },\r\n  mounted(){\r\n    this.init();\r\n  },\r\n  methods: {\r\n    init(){\r\n      console.log(this.$route)\r\n      this.$nextTick( ()  => {\r\n        this.getSong(); \r\n        this.getSingerAlbum(); \r\n        this.getSingerMv();\r\n        this.getSimiSinger();\r\n      })\r\n      \r\n    },\r\n\r\n    // 获取歌手单曲\r\n    async getSong(){\r\n      await this.$api.Artists(this.id)\r\n        .then(res => {\r\n          this.songList = res.data.hotSongs;\r\n          let artist = res.data.artist;\r\n          this.getSingerInfo(artist);\r\n        })\r\n        this.song = this.songList.slice(0,10)\r\n    },\r\n\r\n    // 获取歌手专辑\r\n    async getSingerAlbum(){\r\n      let params = {\r\n        id: this.id,\r\n        limit: 4,\r\n      }\r\n      await this.$api.ArtistAlbum(params)\r\n        .then(res => {\r\n          this.album = res.data.hotAlbums;\r\n        })\r\n    },\r\n\r\n    // 获取歌手mv\r\n    async getSingerMv(){\r\n      await this.$api.ArtistMv(this.id)\r\n        .then(res => {\r\n          this.mv = res.data.mvs.slice(0,4);\r\n        })\r\n    },\r\n\r\n    // 获取相似歌手\r\n    async getSimiSinger(){\r\n      await this.$api.SimiArtist(this.id)\r\n        .then(res => {\r\n          console.log(res)\r\n          this.simi = res.data.artists.slice(0,8);\r\n        })\r\n    },\r\n\r\n    // 获取歌手基本信息\r\n    getSingerInfo(artist){\r\n      this.info = {\r\n        name: artist.name,\r\n        picUrl: artist.picUrl,\r\n        desc: artist.briefDesc,\r\n        album: artist.albumSize,\r\n        music: artist.musicSize,\r\n        mv: artist.mvSize,\r\n      }\r\n    },\r\n\r\n    // 跳转至锚点\r\n    jump(dom){\r\n      document.querySelector(`#${dom}`).scrollIntoView(true);\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n  .singer-info{\r\n    @include shadow;\r\n    margin: 40px 0;\r\n    padding: 20px 0 10px 20px;\r\n    display: flex;\r\n    .cover{\r\n      margin-right: 100px;\r\n      width: 250px;\r\n      height: 250px;\r\n      .el-image{\r\n        border-radius: 50%;\r\n      }\r\n    }\r\n    .info{\r\n      flex: 1;\r\n      padding-right: 20px;\r\n      .name{\r\n        margin-bottom: 10px;\r\n        h1{\r\n          font-weight: 500;\r\n          font-size: 38px;\r\n        }\r\n      }\r\n      .des{\r\n        margin-bottom: 10px;\r\n        font-size: 14px;\r\n      }\r\n      .works{\r\n        display: flex;\r\n        .item{\r\n          padding-right: 10px;\r\n          margin-right: 10px;\r\n          border-right: 1px solid #4a4a4a;\r\n          cursor: pointer;\r\n          span{\r\n            font-size: 18px;\r\n            strong{\r\n              font-size: 25px;\r\n              font-weight: 500;\r\n            }\r\n          }\r\n          &:last-child{\r\n            border-right: none;\r\n          }\r\n          &:hover{\r\n            color: $mainColor;\r\n          }\r\n        }\r\n\r\n      }\r\n    }\r\n  }\r\n  .song,.album,.mv,.simi{\r\n    @include shadow;\r\n    margin-bottom: 20px;\r\n    padding: 10px 0 0 10px;\r\n    .title{\r\n      height: 58px;\r\n      line-height: 58px;\r\n      h2{\r\n        font-weight: 500;\r\n      }\r\n    }\r\n  }\r\n</style>"],"sourceRoot":"src/components/common/singer/singer-detail"}]}